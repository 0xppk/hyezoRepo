{
  "extends": "@hyezo/tsconfig/nextjs.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "worker/index.js",
    ".tmp/createSubscription.ts",
    ".tmp/tsup.config.ts"
  ],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./app/*"],
      "~/*": ["./*"],
      "@hyezo/*": ["../../packages/utils/public/*"]
    },
    "strictNullChecks": true
  }
}
