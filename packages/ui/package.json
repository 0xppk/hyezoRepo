{
  "name": "@hyezo/ui",
  "version": "1.1.0",
  "type": "module",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "public/**"
  ],
  "scripts": {
    "build": "tsup --external react && tailwindcss -i ./src/styles.css -o ./public/styles.css",
    "dev": "concurrently \"tsup --external react\" \"tailwindcss -i ./src/styles.css -o ./public/styles.css --watch\"",
    "clean": "rm -rf public",
    "lint": "TIMING=1 eslint \"./**/*.ts*\""
  },
  "devDependencies": {
    "@hyezo/tsconfig": "workspace:*",
    "@next/font": "13.1.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "concurrently": "^7.6.0",
    "cva": "npm:class-variance-authority@^0.4.0",
    "eslint": "^8.32.0",
    "eslint-config-hyezo": "workspace:*",
    "next": "^13.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-config-hyezo": "workspace:*",
    "tailwindcss": "^3.2.4",
    "tiny-glob": "^0.2.9",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
